<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="wat.edu.pl.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Contact"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Dietetician"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.DietAppointment"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Client"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Dietappointment"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Diet_product"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Diet"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Product"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Product.dietProducts"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Meal"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="wat.edu.pl.domain.Result"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
